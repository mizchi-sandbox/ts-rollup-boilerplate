{
  "parser": "typescript-eslint-parser",
  "plugins": ["prettier"],
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error"
  }
}
